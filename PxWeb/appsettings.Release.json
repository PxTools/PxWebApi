{
  "DataSource": {
    "DataSourceType": "#{DATASOURCETYPE}#",
    "PX": {
      "StrictAggregations": "true"
    },
    "CNMM": {
      "DatabaseID": "#{DATABASEID}#"
    }
  },
  "PxApiConfiguration": {
    "Languages": [
      {
        "Id": "sv",
        "Label": "Svenska"
      },
      {
        "Id": "en",
        "Label": "English"
      }
    ],
    "DefaultLanguage": "en",
    "MaxDataCells": 10000,
    "License": "https://creativecommons.org/share-your-work/public-domain/cc0/",
    "SourceReferences": [
      {
        "Language": "sv",
        "Text": "KÃ¤lla: SCB"
      },
      {
        "Language": "en",
        "Text": "Source: Statistics Sweden"
      }
    ],
    "Cors": {
      "Enabled": true,
      "Origins": "*"
    },
    "CacheTime": 86400,
    "SearchEngine": "Lucene",
    "PageSize": 20,
    "BaseURL": "#{BASEURL}#"
  },
  "AdminProtection": {
    "IpWhitelist": [ "#{ADMIN_IPCONFIG}#" ],
    "AdminKey":  "#{API_ADMIN_KEY}#" 
  },
  "IpRateLimiting": {
    "EnableEndpointRateLimiting": #{ENABLE_ENDPOINT_RATE_LIMITING}#,
    "StackBlockedRequests": false,
    "RealIpHeader": "X-Forwarded-For",
    "ClientIdHeader": "",
    "HttpStatusCode": 429,
    "IpWhitelist": [ "#{IPCONFIG}#" ],
    "EndpointWhitelist": [ "get:/v2/config" ],
    "ClientWhitelist": [],
    "GeneralRules": [
      {
        "Endpoint": "*",
        "Period": "10s",
        "Limit": 30
      }
      //,
      //{
      //  "Endpoint": "*",
      //  "Period": "15m",
      //  "Limit": 100
      //},
      //{
      //  "Endpoint": "*",
      //  "Period": "12h",
      //  "Limit": 1000
      //},
      //{
      //  "Endpoint": "*",
      //  "Period": "7d",
      //  "Limit": 10000
      //}
    ]
  },

  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "config1": ""
}
