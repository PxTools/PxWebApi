{
  "DataSource": {
    "DataSourceType": "#{DATASOURCETYPE}#",
    "PX": {
      "StrictAggregations": "true"
    },
    "CNMM": {
      "DatabaseID": "#{DATABASEID}#"
    }
  },
  "PxApiConfiguration": {
    "Languages": [
      {
        "Id": "sv",
        "Label": "Svenska"
      },
      {
        "Id": "en",
        "Label": "English"
      }
    ],
    "DefaultLanguage": "en",
    "MaxDataCells": #{MAXDATACELLS}#,
    "License": "https://creativecommons.org/share-your-work/public-domain/cc0/",
    "SourceReferences": [
      {
        "Language": "sv",
        "Text": "KÃ¤lla: SCB"
      },
      {
        "Language": "en",
        "Text": "Source: Statistics Sweden"
      }
    ],
    "Cors": {
      "Enabled": true,
      "Origins": "*"
    },
    "CacheTime": 86400,
    "SearchEngine": "Lucene",
    "PageSize": 20,
    "BaseURL": "#{BASEURL}#",
    "OutputFormats": [
      "xlsx",
      "xlsx_doublecolumn",
      "csv",
      "csv_tab",
      "csv_tabhead",
      "csv_comma",
      "csv_commahead",
      "csv_space",
      "csv_spacehead",
      "csv_semicolon",
      "csv_semicolonhead",
      "csv2",
      "csv3",
      "json",
      "json-stat",
      "json-stat2",
      "html5_table",
      "relational_table",
      "px"
    ],
    "DefaultOutputFormat": "px",
    "EnableSwaggerUI": false
  },
  "LuceneConfiguration": {
    "IndexDirectory": "Database"
  },
  "AdminProtection": {
    "IpWhitelist": [ "#{ADMIN_IPCONFIG}#" ],
    "AdminKey":  "#{API_ADMIN_KEY}#" 
  },
  "IpRateLimiting": {
    "EnableEndpointRateLimiting": #{ENABLE_ENDPOINT_RATE_LIMITING}#,
    "StackBlockedRequests": false,
    "RealIpHeader": "X-Forwarded-For",
    "ClientIdHeader": "",
    "HttpStatusCode": 429,
    "IpWhitelist": [ "#{IPCONFIG}#" ],
    "EndpointWhitelist": [ "get:/v2/config" ],
    "ClientWhitelist": [],
    "GeneralRules": [
      {
        "Endpoint": "*",
        "Period": "10s",
        "Limit": 30
      }
      //,
      //{
      //  "Endpoint": "*",
      //  "Period": "15m",
      //  "Limit": 100
      //},
      //{
      //  "Endpoint": "*",
      //  "Period": "12h",
      //  "Limit": 1000
      //},
      //{
      //  "Endpoint": "*",
      //  "Period": "7d",
      //  "Limit": 10000
      //}
    ]
  },

  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "config1": ""
}
